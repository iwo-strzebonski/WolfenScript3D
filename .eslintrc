{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "strict": [
      "error",
      "global"
    ],
    "indent": [
      "error",
      4
    ],
    "max-len": [
      "error",
      79
    ],
    "eol-last": [
      "error",
      "always"
    ],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 2,
        "maxEOF": 0,
        "maxBOF": 0
      }
    ],
    "@typescript-eslint/no-non-null-assertion": "off",
    "linebreak-style": [
      "error",
      "windows"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "never"
    ],
    "require-jsdoc": [
      "error",
      {
        "require": {
          "FunctionDeclaration":true,
          "MethodDefinition":true,
          "ClassDeclaration":true
        }
      }
    ],
    "no-var": "error",
    "no-eval": "error",
    "no-console": [
      "warn"
    ],
    "space-before-function-paren": [
      "error",
      "never"
    ],
    "padded-blocks": [
      "error",
      "never"
    ],
    "prefer-arrow-callback": [
      0,
      {
        "allowNamedFunctions":true
      }
    ],
    "func-names": [
      "error",
      "never"
    ],
    "no-use-before-define": [
      "error",
      {
        "functions":true,
        "classes":true
      }
    ],
    "max-nested-callbacks": [
      "error",
      5
    ]
  },
  "globals": {}
}